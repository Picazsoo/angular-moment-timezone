{
  "name": "angular-moment-timezone",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saaadel/angular-moment-timezone.git"
  },
  "author": "Andrew Spoda",
  "license": "MIT",
  "keywords": [
    "angular2",
    "angular4",
    "momentjs",
    "moment",
    "moment-timezone"
  ],
  "bugs": {
    "url": "https://github.com/saaadel/angular-moment-timezone/issues"
  },
  "homepage": "https://github.com/saaadel/angular-moment-timezone#readme",
  "main": "index.js",
  "typings": "./index.d.ts",
  "files": [
    "**/*"
  ],
  "scripts": {
    "prepublish": "del-cli -f src/**/*.ngfactory.* src/**/*.ngsummary.* src/node_modules/ out-tsc/ && ngc -p src/tsconfig.demo.json && copyfiles -f out-tsc/demo/angular-moment-timezone/*.d.ts out-tsc/demo/angular-moment-timezone/*.js out-tsc/demo/angular-moment-timezone/*.js.map out-tsc/demo/angular-moment-timezone/*.metadata.json src/angular-moment-timezone/*.ngfactory.* src/angular-moment-timezone/*.ngsummary.* !out-tsc/**/*.spec.d.ts !out-tsc/**/*.spec.js ./ && del-cli -f src/**/*.ngfactory.* src/**/*.ngsummary.* src/node_modules/ out-tsc/",
    "ng": "ng",
    "ngc": "ngc",
    "ngc:clean": "del-cli -f src/**/*.ngfactory.* src/**/*.ngsummary.* src/node_modules/",
    "src:js:clean": "del-cli -f src/*/**/*.js src/main.js src/polyfills.js src/test.js",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.2.34",
    "angular2-moment": "^1.3.3",
    "core-js": "^2.4.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "copyfiles": "1.0.0",
    "del-cli": "^1.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
